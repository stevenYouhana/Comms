<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="AnchorPane" prefHeight="230.0" prefWidth="250.0" 
            xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" vgap="10"
fx:controller="UI.FXMLSerialSettingsController">
    <Label text="*Baud Rate" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Label text="Data bits" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Label text="Stop bits" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <Label text="Parity" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Label text="Flow control" GridPane.columnIndex="0" GridPane.rowIndex="4" prefWidth="70" />
    <TextField fx:id="txtBRate" GridPane.columnIndex="1" GridPane.rowIndex="0" prefWidth="70" />
    <TextField fx:id="txtDBits" GridPane.columnIndex="1" GridPane.rowIndex="1" prefWidth="70" />
    <TextField fx:id="txtSBits" GridPane.columnIndex="1" GridPane.rowIndex="2" prefWidth="70" />
    <ComboBox fx:id="cboParity" GridPane.columnIndex="1" GridPane.rowIndex="3" prefWidth="90" />
    <ComboBox fx:id="cboFCtrl" GridPane.columnIndex="1" GridPane.rowIndex="4" prefWidth="90" />
    <Button fx:id="btnSet" onAction="#set" text="Connect" GridPane.columnIndex="1" 
            GridPane.rowIndex="6" prefWidth="70 "/>
</GridPane>
